#!/data/data/com.termux/files/usr/bin/bash
##
##  Find all files which are not owned by any package.
##
##  Useful if you decide to clean your installation from
##  various third-party stuff.
##
##  Dependencies: bash, busybox
##

: "${PREFIX:=/data/data/com.termux/files/usr}"
comm -23 \
    <(find $PREFIX | \
        grep -v "$PREFIX/etc/apt/apt.conf.d$" | \
        grep -v "$PREFIX/etc/apt/preferences.d$" | \
        grep -v "$PREFIX/run$" | \
        grep -v "$PREFIX/tmp$" | \
        grep -v "$PREFIX/var/lib/dpkg/info" | \
        grep -v "$PREFIX/var/lib/dpkg/lock" | \
        grep -v "$PREFIX/var/lib/dpkg/status" | \
        grep -v "$PREFIX/var/lib/dpkg/triggers" | \
        grep -v "$PREFIX/var/lib/dpkg/updates" | \
      sort) \
    <(cat $PREFIX/var/lib/dpkg/info/*.list | \
        grep $PREFIX | \
        grep -v "$PREFIX/etc/apt/apt.conf.d$" | \
        grep -v "$PREFIX/etc/apt/preferences.d$" | \
        grep -v "$PREFIX/run$" | \
        grep -v "$PREFIX/tmp$" | \
        grep -v "$PREFIX/var/lib/dpkg/info" | \
        grep -v "$PREFIX/var/lib/dpkg/lock" | \
        grep -v "$PREFIX/var/lib/dpkg/status" | \
        grep -v "$PREFIX/var/lib/dpkg/triggers" | \
        grep -v "$PREFIX/var/lib/dpkg/updates" | \
      sort | uniq)
